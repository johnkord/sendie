{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.SignalR": "Debug",
      "Sendie": "Debug"
    }
  },
  "Discord": {
    "ClientId": "YOUR_DISCORD_CLIENT_ID",
    "ClientSecret": "YOUR_DISCORD_CLIENT_SECRET"
  },
  "AccessControl": {
    "Admins": [
      "YOUR_DISCORD_USER_ID"
    ],
    "InitialAllowList": []
  }
}
